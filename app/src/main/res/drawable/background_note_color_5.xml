<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Larger blue circle -->
    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="@dimen/_4sdp"
                android:left="@dimen/_4sdp"
                android:right="@dimen/_4sdp"
                android:top="@dimen/_4sdp" />
            <size
                android:width="@dimen/_35sdp"
                android:height="@dimen/_35sdp" />
            <stroke
                android:width="@dimen/_5sdp"
                android:color="@color/colorNoteColor5" />
        </shape>
    </item>
    <!-- Green circle in middle -->
    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="@dimen/_2sdp"
                android:left="@dimen/_2sdp"
                android:right="@dimen/_2sdp"
                android:top="@dimen/_2sdp" />
            <size
                android:width="@dimen/_35sdp"
                android:height="@dimen/_35sdp" />
            <stroke
                android:width="@dimen/_4sdp"
                android:color="@color/colorMiscellaneousBackground" />
        </shape>
    </item>
    <!-- Smaller red circle at front -->
    <item>
        <shape android:shape="oval">
            <size
                android:width="@dimen/_35sdp"
                android:height="@dimen/_35sdp" />
            <solid android:color="@color/colorNoteColor5" />
        </shape>
    </item>
</layer-list>
